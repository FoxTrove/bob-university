{
  "expo": {
    "name": "bob-university",
    "slug": "bob-university",
    "scheme": "bob-university",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thebobco.bobuniversity",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSLocalNetworkUsageDescription": "This app uses the local network to connect to the development server."
      }
    },
    "android": {
      "package": "com.foxtrove.bobuniversity",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-notifications",
      "expo-apple-authentication",
      "@react-native-google-signin/google-signin",
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox"
        }
      ],
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "30ff9fd3-6d93-4f3e-8588-d1776e1525a5"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/30ff9fd3-6d93-4f3e-8588-d1776e1525a5"
    }
  }
}
